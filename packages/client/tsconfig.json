{
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "isolatedModules": false,
    "moduleResolution": "node",
    "noEmitOnError": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "strict": true,
    "jsx": "react",
    "lib": ["dom", "dom.iterable", "esnext"],
    "sourceMap": true,
    "noImplicitAny": true,
    "module": "esnext",
    "outDir": "dist",
    "target": "es5",
    "rootDirs": ["src"],
    "baseUrl": "."
  }
}
