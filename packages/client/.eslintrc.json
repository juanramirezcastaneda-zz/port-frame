{
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "plugins": ["filenames"],
  "rules": {
    "filenames/match-regex": [2, "^[A-Z][a-z]+(?:[A-Z][a-z]+)*$", true],
    "filenames/match-exported": 2,
    "filenames/no-index": 2
  }
}
